    {
        "id": "b25d19969a919679",
        "type": "function",
        "z": "tab_ritual_wifi",
        "name": "ToDMXFixtures",
        "func": "// get the array from Arduino\nvar statusArray = msg.payload.data;\nvar outputMsgs = [];\n\n// Get the previous state (if none, default to all 0)\nvar lastStates = context.get('lastStates') || [0, 0, 0, 0, 0];\n\nfor (var i = 0; i < 5; i++) {\n    // send only if the state is changed\n    if (statusArray[i] != lastStates[i]) {\n\n        var oscPath = \"/ritual/light/\" + (i + 1);\n        \n        var oscValue = (statusArray[i] == 1) ? 1 : 0;\n\n        outputMsgs.push({\n            topic: oscPath,\n            payload: oscValue\n        });\n    }\n}\n\n// store the current state\ncontext.set('lastStates', statusArray);\n\n// if the array is empty, nothing changes, do not send\nif (outputMsgs.length === 0) {\n    return null;\n}\n\nreturn [outputMsgs];",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
