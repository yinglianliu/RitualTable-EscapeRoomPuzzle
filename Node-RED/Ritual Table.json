    {
        "id": "split_function",
        "type": "function",
        "z": "tab_ritual_wifi",
        "name": "SplitStateData",
        "func": "// input data format: { \"data\": [1,0,1,0,0], \"solved\": false }\n\nvar states = msg.payload.data;\nvar isSolved = msg.payload.solved;\n\nfunction toBool(val) {\n    return val == 1;\n}\n\n// send to 5 LED\nvar msg1 = { payload: toBool(states[0]) };\nvar msg2 = { payload: toBool(states[1]) };\nvar msg3 = { payload: toBool(states[2]) };\nvar msg4 = { payload: toBool(states[3]) };\nvar msg5 = { payload: toBool(states[4]) };\n\n// \nvar msgSolved = { payload: isSolved ? \"PUZZLE SOLVED!\" : \"Waiting...\" };\n\nreturn [msg1, msg2, msg3, msg4, msg5, msgSolved];",
        "outputs": 6,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 480,
        "y": 200,
        "wires": [
